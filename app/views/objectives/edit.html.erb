<h1>objective</h1>

<%= form_for(@objective) do |@f| %>
  <%= render 'form' %>
  <div class="actions">
    <%= @f.submit "Update" %>
    <%= link_to "Delete", @objective,
      :method => :delete,
      :confirm => "削除します。よろしいですか?",
      :title => "Delete #{@objective.name}"
    %>
  </div>
<% end %>
